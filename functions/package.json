{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase --project atm-voting serve --only functions",
    "shell": "firebase --project atm-voting functions:shell",
    "start": "npm run shell",
    "deploy": "firebase --project atm-voting deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "firebase-admin": "^11.4.1",
    "firebase-functions": "^3.24.1",
    "mailgun-js": "^0.22.0",
    "memory-stream": "^1.0.0",
    "qrcode": "^1.4.1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
